name = "John Doe"
print(name)

street = 'Neverland Lane'
print(street)

items = 5
print(items)

price = 19.95
print(price)

students = 28
teachers = 3
seats = students + teachers
print(seats)

correct = students > teachers
print(correct)

incorrect = students > seats
print(incorrect)

line_character = '-'
line = line_character * 20
print(line)
